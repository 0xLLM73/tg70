{
  "name": "@cabal-ventures/bot",
  "version": "0.1.0",
  "description": "Cabal.Ventures Telegram Bot Core",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:interceptor": "jest --config jest.interceptor.config.js",
    "test:interceptor:watch": "jest --config jest.interceptor.config.js --watch",
    "test:demo": "tsx src/__tests__/interceptor/mock-vs-interceptor-demo.ts",
    "test:all": "npm run test && npm run test:interceptor",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/validator": "^13.15.2",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^4.0.1",
    "sanitize-html": "^2.11.0",
    "telegraf": "^4.15.6",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/sanitize-html": "^2.9.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0"
  }
}